    "version": "0.2.0",
    "configurations": [
    {
        "name": "(gdb) FRISC-fnsim",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/ucle/build/core/debug/frisc-fnsim",
        "args": ["ucle/core/tests/hls/test_cmp.p"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
